{
  "name": "react-clipboard-icon",
  "version": "0.2.0",
  "description": "is an svg icon from clipboard.js site assets, packaged as a React stateless component",
  "main": "index.js",
  "scripts": {
    "babel": "babel ClipboardIcon.js -o index.js",
    "postupdate-deps": "npm test && git commit -am 'updated deps' || git checkout -- package.json",
    "postversion": "git push origin v${npm_package_version}; npm publish",
    "test": "echo \"Error: no test specified\" && exit 1",
    "update-deps": "npm update --save --dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fibo/react-clipboard-icon.git"
  },
  "keywords": [
    "react",
    "svg",
    "clipboard",
    "icon"
  ],
  "author": {
    "name": "Gianluca Casati",
    "url": "http://g14n.info"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fibo/react-clipboard-icon/issues"
  },
  "homepage": "http://npm.im/react-clipboard-icon",
  "dependencies": {
    "react": "^15.1.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-cli": "^6.10.1"
  }
}
